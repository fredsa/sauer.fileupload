<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">

  <servlet>
    <servlet-name>upload-servlet</servlet-name>
    <servlet-class>demo.BaseUploadServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>upload-servlet</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>


  <!-- Blob upload servlet -->
  <servlet>
    <servlet-name>blob-upload-servlet</servlet-name>
    <servlet-class>demo.BlobUploadServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>blob-upload-servlet</servlet-name>
    <url-pattern>/blob-upload</url-pattern>
  </servlet-mapping>


  <!-- Plain file upload servlet -->
  <servlet>
    <servlet-name>file-upload-servlet</servlet-name>
    <servlet-class>demo.FileUploadServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>file-upload-servlet</servlet-name>
    <url-pattern>/file-upload</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
